{
  "name": "secure-contact-manager",
  "scripts": {
    "start:renderer": "snowpack dev --open none",
    "build:main": "tsc -p tsconfig.main.json",
    "watch:main": "yarn build:main -w",
    "start:main": "yarn build:main && concurrently \"yarn watch:main\" \"electron ./build/main/main\"",
    "start": "concurrently \"yarn start:renderer\" \"wait-on http://localhost:8080  && yarn start:main\"",
    "test:main": "jest"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.3",
    "electron": "^11.3.0",
    "electron-reloader": "^1.2.0",
    "flexsearch": "^0.6.32",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-use-flexsearch": "^0.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@snowpack/app-scripts-react": "^2.0.0",
    "@snowpack/plugin-babel": "^2.1.6",
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-react-refresh": "^2.4.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/snowpack-env": "^2.3.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "@yarnpkg/pnpify": "^2.4.0",
    "concurrently": "^6.0.0",
    "electron-reload": "^1.5.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "faker": "^5.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rollup-plugin-pnp-resolve": "^2.0.0",
    "snowpack": "^3.0.11",
    "tmp-promise": "^3.0.2",
    "ts-jest": "^26.5.2",
    "typescript": "^4.1.5",
    "wait-on": "^5.2.1"
  }
}
