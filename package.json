{
  "name": "secure-contact-manager",
  "main": "",
  "scripts": {
    "start:renderer": "snowpack dev --open none",
    "build:main": "tsc ./src/main/**/*.ts --outDir ./build",
    "watch:main": "yarn build:main --incremental --tsBuildInfoFile ./tsbuildinfo -w",
    "start:main": "yarn build:main && concurrently \"yarn watch:main\" \"electron ./build/main\""
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.3",
    "electron": "^11.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@snowpack/app-scripts-react": "^2.0.0",
    "@snowpack/plugin-babel": "^2.1.6",
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-react-refresh": "^2.4.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/snowpack-env": "^2.3.3",
    "@yarnpkg/pnpify": "^2.4.0",
    "concurrently": "^6.0.0",
    "prettier": "^2.2.1",
    "rollup-plugin-pnp-resolve": "^2.0.0",
    "snowpack": "^3.0.11",
    "typescript": "^4.1.5"
  }
}
