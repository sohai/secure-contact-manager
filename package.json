{
  "name": "secure-contact-manager",
  "scripts": {
    "start:renderer": "snowpack dev --open none",
    "build:main": "tsc -p tsconfig.main.json",
    "watch:main": "yarn build:main -w",
    "start:main": "yarn build:main && concurrently \"yarn watch:main\" \"electron ./build/main/main\"",
    "start:electron": "electron ./build/main/main",
    "start": "concurrently \"yarn start:renderer\" \"yarn wait-on http://localhost:8080  && yarn start:main\""
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.3",
    "electron": "^11.3.0",
    "electron-reloader": "^1.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@snowpack/app-scripts-react": "^2.0.0",
    "@snowpack/plugin-babel": "^2.1.6",
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-react-refresh": "^2.4.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/snowpack-env": "^2.3.3",
    "@types/uuid": "^8.3.0",
    "@yarnpkg/pnpify": "^2.4.0",
    "concurrently": "^6.0.0",
    "electron-reload": "^1.5.0",
    "prettier": "^2.2.1",
    "rollup-plugin-pnp-resolve": "^2.0.0",
    "snowpack": "^3.0.11",
    "typescript": "^4.1.5"
  }
}
